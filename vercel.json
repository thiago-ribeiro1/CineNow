{
  "version": 2,
  "builds": [
    { "src": "src/app.js", "use": "@vercel/node" },
    { "src": "public/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/movies", "dest": "/src/app.js" },
    { "src": "/news", "dest": "/src/app.js" },
    { "src": "/news/(.*)", "dest": "/src/app.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
